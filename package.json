{
  "name": "json-ultra-compress",
  "version": "1.1.0",
  "description": "JSON-native compression with selective field decode. Beats Brotli/Zstd on structured logs & APIs.",
  "license": "MIT",
  "type": "module",
  "homepage": "https://github.com/fayez-kaabi/json-ultra-compress#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/fayez-kaabi/json-ultra-compress.git"
  },
  "bugs": {
    "url": "https://github.com/fayez-kaabi/json-ultra-compress/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "json-ultra-compress": "dist/cli.js"
  },
  "engines": {
    "node": ">=18"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./cli": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "bench": "tsx scripts/bench.ts",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "json",
    "ndjson",
    "compression",
    "columnar",
    "selective-decode",
    "brotli",
    "gzip",
    "zstd",
    "logs",
    "analytics",
    "typescript"
  ],
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "globby": "^14.0.1",
    "undici": "^6.19.8"
  },
  "optionalDependencies": {
    "@zstd/wasm": "^0.9.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "tsx": "^4.15.7",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4"
  }
}
